<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.StreetwiseWizard" name="Streetwise Wizard" version="2.1" provider-name="Streetwise">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Streetwise Wizard</summary>
    <description lang="en">This Wizard allows you to get access to the latest Streetwise Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>

